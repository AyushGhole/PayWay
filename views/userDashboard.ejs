<% layout("/layouts/sidebar1.ejs") %>

  <div class="thank-you-container">
    <% if(bank.length==0 ){ %>
      <!-- Account Balance Declaration -->
      <h1>
        <a class="btn btn-primary" id="l=SignUp" href="/main/<%= currUser._id %>/add_bank"><small>Add Bank</small></a>
      </h1>
      <p>Account Balance </p>

      <% } %>

        <% if(bank.length>0 ) { %>
          <!-- Account Balance Declaration -->
          <h1>
            &#8377;<%= bankDetails.Amount.toLocaleString("en-IN") %>
          </h1>
          <p>Account Balance </p>

          <% } %>

            <!-- Upi Id Declaration -->
            <p>Upi Id : &nbsp;<span id="order-number">
                <b>
                  <%=User.username %>@okhfdc
                </b>
              </span></p>

            <hr />
            <% if(trans.length==0 ){ %>
              <div class="emoji"></div>
              <p>No Transactions Yet !!!</p>

              <% } %>

                <!-- Transaction Section  -->
                <% if(trans.length !==0){ %>
                  <p><b> Recent Transactions</b></p>
                  <% for(transact of populates){ %>
                    <% if(transact.Status=="success" ) {%>
                      <div class="thank-you-container transact">
                        <%= transact.AccountHolderName %>&nbsp; &nbsp; &nbsp; &nbsp;
                          <%= transact.Amount %>&nbsp; &nbsp; &nbsp; &nbsp;
                            <i class="fa-solid fa-check" style="color: green;"></i>
                      </div>
                      <% } %>
                        <% if(transact.Status=="failed" ){ %>
                          <div class="thank-you-container transact">
                            <%= transact.AccountHolderName %>&nbsp; &nbsp; &nbsp; &nbsp;
                              <%= transact.Amount %>&nbsp; &nbsp; &nbsp; &nbsp;
                                <i class="fa-solid fa-circle-exclamation" style="color: brown;"></i>
                          </div>
                          <% } %>
                            <% } %>

                              <% } %>

  </div>


